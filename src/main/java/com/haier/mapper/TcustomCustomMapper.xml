<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.haier.mapper.TcustomCustomMapper">
    <select id="selectCustomUsers" resultType="java.lang.String">
        SELECT DISTINCT (c.`userName`)
        FROM tcustom c
        WHERE c.`status` > 0
    </select>

</mapper>